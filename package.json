  "scripts": {
    "start": "node ./dist/server.js",
    "test": "jest --c config/jest.config.ts",
    "develop": "rm -rf ./dist && TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling APP_ENV=development NODE_ENV=development node_modules/.bin/webpack --config=config/webpack.config.js",
    "backup:sync": "./scripts/sync-backup.sh",
    "backup:restore": "./scripts/restore-backup.sh",
    "build:production": "rm -rf ./dist && APP_ENV=production NODE_ENV=production node_modules/.bin/webpack --mode=production --config=config/webpack.config.js",
    "migration:revert": "node_modules/.bin/ts-node --project tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert --config src/server/ormconfig.js",
    "migration:run": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node_modules/.bin/ts-node --project tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run --config src/server/ormconfig.js",
    "migration:generate": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node_modules/.bin/ts-node --project tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate --config src/server/ormconfig.js",
    "sitemap:refresh": "node_modules/.bin/gulp sitemap:refresh",
    "schema:sync": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node_modules/.bin/ts-node --project tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm schema:sync --config src/server/ormconfig.js",
